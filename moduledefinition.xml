<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="http://www.webhare.net/xmlns/system/moduledefinition">

  <meta>
    <version>1.0</version>
    <description>Webwerf</description>
    <author>Wouter Hendriks</author>
    <packaging download="true"/>
  </meta>

  <adhocdumps>
    <adhocdump tag="contactform" title="Webwerf contact formulier" />
  </adhocdumps>

  <publisher>
    <webdesign name="webwerf" title="Webwerf" siteprofile="webwerf.siteprl">
      <assetpack entrypoint="webdesigns/webwerf/js/webwerf" supportedlanguages="nl" />
    </webdesign>
  </publisher>

  <services>
    <webservice name="contactform" transports="jsonrpc" library="rpc.whlib" requirewhaccount="false" primarytrans="auto">
      <accesscheck />
    </webservice>
  </services>

</module>
